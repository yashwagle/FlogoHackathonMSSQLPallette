{
  "name": "MSSQLPalette",
  "version": "0.0.1",
  "type": "flogo:activity",
  "title": "MSSQL activity",
  "description": "MS SQL Connector",
  "ref":"github.com/yashwagle/HelloWorld/MSSQLPalette",
  "author": "Team Dummies",
  "inputs":[
    {
      "name": "host",
      "type": "string",
        "required": true
    },
    {
      "name": "method",
      "type": "string",
      "allowed": [
        "Select",
        "DML",
        "Create"
      ],
      "value": "Select",
      "required": true
    },

    {
      "name": "port",
      "type": "string",
        "required": true
    },
    {
      "name": "dbname",
      "type": "string",
        "required": true
    },
    {
      "name": "username",
      "type": "string",
        "required": true
    },
    {
      "name": "password",
      "type": "string",
        "required": true
    },
    {
      "name": "query",
      "type": "string",
        "required": true
    }
  ],
  "outputs": [
    {
      "name": "output",
      "type": "object"
    }
  ]
}
